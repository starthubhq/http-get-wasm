{
  "name": "http-get-wasm",
  "description": "HTTP GET request module for fetching data from web APIs",
  "version": "0.0.16",
  "kind": "wasm",
  "manifest_version": 1,
  "repository": "github.com/starthubhq/http-get-wasm",
  "license": "MIT",
  "inputs": [
    {
      "name": "url",
      "description": "The URL to fetch data from",
      "type": "string",
      "required": true,
      "default": null
    },
    {
      "name": "headers",
      "description": "Optional HTTP headers to send with the request",
      "type": "object",
      "required": false,
      "default": null
    }
  ],
  "outputs": [
    {
      "name": "response",
      "description": "HTTP response from the request",
      "type": "HttpResponse",
      "required": true
    }
  ],
  "types": {
    "HttpResponse": {
      "body": "object",
      "status": "number"
    }
  },
  "permissions": {
    "net": ["http", "https"]
  },
  "mirrors": [
    "https://api.starthub.so/storage/v1/object/public/artifacts/std/http-get-wasm/0.0.1/artifact.zip"
  ]
}