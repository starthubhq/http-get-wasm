{
  "name": "http-get-wasm",
  "version": "0.0.1",
  "kind": "wasm",
  "repository": "github.com/starthubhq/http-get-wasm",
  "image": "ghcr.io/starthubhq/http-get-wasm",
  "license": "MIT",
  "inputs": [
    { "name": "url", "description": "The URL to GET", "type": "string" },
    { "name": "headers", "description": "Optional HTTP headers (object of key->value)", "type": "object" }
  ],
  "outputs": [
    { "name": "status", "description": "HTTP status code", "type": "integer" },
    { "name": "body",   "description": "Response body as string", "type": "string" }
  ]
}